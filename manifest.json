{
  "manifest_version": 3,
  "name": "JS Miner",
  "version": "1.0",
  "description": "Fetch .js files from the current page and find URLs or endpoints.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
